{
  "name": "@azify/aziface-mobile",
  "version": "2.0.0",
  "description": "A Aziface adapter to react native. ðŸ“±",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "podspec": "pod ipc spec AzifaceMobile.podspec",
    "release": "npx semantic-release --ci",
    "prepare": "bob build",
    "prepack": "bob build",
    "setup": "npm i && npm run podspec && npm run prepack",
    "android": "npm run setup && cd example && npm i && npm run start",
    "ios": "npm run setup && cd example && npm i && cd ios && pod install && cd .. && npm run start"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "azify",
    "aziface"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azifydev/aziface-mobile.git"
  },
  "author": "Azify <nayaradias743@gmail.com> (https://github.com/azifydev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azifydev/aziface-mobile/issues"
  },
  "homepage": "https://github.com/azifydev/aziface-mobile#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@evilmartians/lefthook": "^1.12.3",
    "@react-native-community/cli": "20.0.1",
    "@react-native/babel-preset": "0.81.1",
    "@react-native/eslint-config": "^0.81.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "10.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "10.0.4",
    "@semantic-release/release-notes-generator": "11.0.4",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.0",
    "commitlint": "^19.8.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "react": "19.1.0",
    "react-native": "0.81.1",
    "react-native-builder-bob": "^0.40.13",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "npm@10.9.2",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "AzifaceMobileSpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.azifacemobile"
    },
    "ios": {
      "componentProvider": {
        "AzifaceMobile": "AzifaceMobile"
      }
    }
  },
  "create-react-native-library": {
    "languages": "kotlin-objc",
    "type": "turbo-module",
    "version": "0.54.3"
  },
  "react-native": {
    "android": {
      "sourceDir": "../android",
      "packageImportPath": "import com.azifacemobile.AzifaceMobilePackage;"
    },
    "ios": {
      "podspecPath": "./AzifaceMobile.podspec",
      "podspecName": "AzifaceMobile"
    }
  }
}
